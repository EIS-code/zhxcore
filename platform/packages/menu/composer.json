{
    "name": "zhxcore/menu",
    "description": "Menu package for ZHXCore CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "ZHXCore\\Menu\\": "src"
        }
    },
    "require": {
        "zhxcore/platform": "*@dev",
        "zhxcore/theme": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZHXCore\\Menu\\Providers\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "ZHXCore\\Menu\\Facades\\MenuFacade"
            }
        }
    }
}
