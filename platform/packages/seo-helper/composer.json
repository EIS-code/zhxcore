{
    "name": "zhxcore/seo-helper",
    "description": "SEO helper package for ZHXCore CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "ZHXCore\\SeoHelper\\": "src"
        }
    },
    "require": {
        "zhxcore/platform": "*@dev",
        "zhxcore/theme": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZHXCore\\SeoHelper\\Providers\\SeoHelperServiceProvider"
            ],
            "aliases": {
                "SeoHelper": "ZHXCore\\SeoHelper\\Facades\\SeoHelperFacade"
            }
        }
    }
}
