{
    "name": "zhxcore/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "zhxcore",
        "zhxcore cms",
        "zhxcore platform",
        "zhxcore assets"
    ],
    "homepage": "https://zhxscript.com",
    "authors": [
        {
            "name": "William Troiano",
            "email": "zhxscript@outlook.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.6|^6|^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ZHXCore\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZHXCore\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZHXCore\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "ZHXCore\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
